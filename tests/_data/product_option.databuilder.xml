    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="spryker:databuilder-01 http://static.spryker.com/databuilder-01.xsd"
>

    <transfer name="ProductOptionGroup">
        <property name="fkTaxSet" dataBuilderRule="=1"/>
        <property name="active" dataBuilderRule="=true"/>
        <property name="name" dataBuilderRule="lexify('??????????')"/>
    </transfer>

    <transfer name="ProductOptionTranslation">
        <property name="key" dataBuilderRule="lexify('??????????')"/>
        <property name="localeCode" dataBuilderRule="=en_US"/>
        <property name="name" dataBuilderRule="lexify('??????????')"/>
    </transfer>

    <transfer name="ProductOptionValue">
        <property name="value" dataBuilderRule="lexify('??????????')"/>
        <property name="sku" dataBuilderRule="unique()->isbn10()"/>
    </transfer>

</transfers>
